{
  "name": "@tsed/mongoose-example",
  "version": "1.0.0",
  "description": "Here an example project with Mongoose and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run clean && npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "seed": "ts-node scripts/seed/index.ts",
    "start": "cross-env NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "npm run build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.36.2",
    "@tsed/core": "5.36.2",
    "@tsed/di": "5.36.2",
    "@tsed/mongoose": "5.36.2",
    "@tsed/swagger": "5.36.2",
    "@tsed/testing": "5.36.2",
    "@tsed/testing-mongoose": "5.36.2",
    "@types/swagger-schema-official": "2.0.20",
    "bcrypt": "^3.0.7",
    "bluebird": "^3.7.2",
    "body-parser": "1.19.0",
    "chalk": "^3.0.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.4",
    "cross-env": "6.0.3",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "5.7.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "rotating-file-stream": "^2.0.1",
    "serve-static": "^1.13.1",
    "ts-httpexceptions": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.2.5",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.17.2",
    "@types/http-proxy": "^1.17.1",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "5.2.7",
    "@types/mongoose": "5.5.32",
    "@types/node": "12.12.9",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/request-promise": "^4.1.42",
    "@types/sinon": "^7.5.0",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^5.0.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.2",
    "nodemon": "1.19.4",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "supertest": "4.0.2",
    "ts-node": "8.5.2",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  }
}
