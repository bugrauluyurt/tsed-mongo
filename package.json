{
    "name": "@tsed/mongoose-example",
    "version": "1.0.0",
    "description": "Here an example project with Mongoose and Ts.ED framework.",
    "scripts": {
        "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
        "build": "npm run tsc",
        "format": "prettier --write ./src/*.ts",
        "test": "npm run clean && npm run test:lint && npm run test:coverage",
        "fix-ts": "eslint --fix './src/*.ts'",
        "test:unit": "cross-env NODE_ENV=test mocha",
        "test:coverage": "cross-env NODE_ENV=test nyc mocha",
        "test:lint": "tslint --project tsconfig.json",
        "test:lint:fix": "tslint --project tsconfig.json --fix",
        "travis:deploy-once": "travis-deploy-once",
        "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
        "tsc": "tsc --project tsconfig.json",
        "tsc:w": "tsc --project tsconfig.json -w",
        "seed": "ts-node scripts/seed/index.ts",
        "start": "cross-env NODE_ENV=development nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
        "start:prod": "cross-env NODE_ENV=production node dist/index.js",
        "docker:build": "npm run build && docker-compose build",
        "deploy": "exit 0"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@tsed/ajv": "^5.53.1",
        "@tsed/common": "^5.53.1",
        "@tsed/core": "^5.53.1",
        "@tsed/di": "^5.53.1",
        "@tsed/mongoose": "^5.53.1",
        "@tsed/passport": "^5.53.1",
        "@tsed/swagger": "^5.53.1",
        "@tsed/testing": "^5.53.1",
        "@tsed/testing-mongoose": "^5.53.1",
        "@types/swagger-schema-official": "^2.0.21",
        "ajv": "^6.12.2",
        "bcrypt": "^3.0.8",
        "bluebird": "^3.7.2",
        "body-parser": "1.19.0",
        "chalk": "^3.0.0",
        "compression": "1.7.4",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "1.4.4",
        "cors": "^2.8.5",
        "cross-env": "6.0.3",
        "dotenv": "^8.2.0",
        "express": "4.17.1",
        "express-session": "1.17.0",
        "helmet": "^3.22.0",
        "lodash": "^4.17.15",
        "method-override": "^3.0.0",
        "moment": "^2.25.3",
        "mongoose": "^5.9.14",
        "morgan": "^1.10.0",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-google-oauth": "^2.0.0",
        "passport-http": "^0.3.0",
        "passport-local": "^1.0.0",
        "rotating-file-stream": "^2.1.1",
        "serve-static": "^1.13.1",
        "ts-httpexceptions": "^4.1.0",
        "ts-json-properties": "^2.0.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^3.0.0",
        "@types/chai": "4.2.5",
        "@types/chai-as-promised": "^7.1.0",
        "@types/express": "^4.17.6",
        "@types/faker": "^4.1.12",
        "@types/helmet": "0.0.46",
        "@types/http-proxy": "^1.17.4",
        "@types/lodash": "^4.14.151",
        "@types/mocha": "5.2.7",
        "@types/mongoose": "^5.7.20",
        "@types/node": "12.12.9",
        "@types/passport": "^1.0.3",
        "@types/passport-local": "^1.0.33",
        "@types/request-promise": "^4.1.46",
        "@types/sinon": "^7.5.2",
        "@types/sinon-chai": "3.2.3",
        "@types/supertest": "2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "chai": "4.2.0",
        "chai-as-promised": "^7.1.1",
        "concurrently": "^5.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "faker": "^4.1.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.2",
        "mocha": "^6.2.3",
        "nodemon": "^2.0.4",
        "nyc": "^14.1.1",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "sinon": "^7.5.0",
        "sinon-chai": "^3.5.0",
        "supertest": "4.0.2",
        "ts-node": "8.5.2",
        "tslint": "5.20.1",
        "typescript": "3.7.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "git add"
        ]
    }
}
